<relations>
<relation id="2d1d5d05-2a48-4681-855b-37bd8db6e458" shortName="MEW1" inObj="WS:double;WHGR:double" relationType="ifclause" description="wind speed >30m/s for 3min with wave height growth rate>0.5m/h (WS >30 &amp; WHGR > 0.5)" outObj="RESULT:string">if (WS > 30 &amp; WHGR > 0.5) {RESULT = 'Trigger red alert and activate emergency mooring';} else {RESULT = 'Normal';}</relation>
<relation id="3c4bd0e6-105b-487d-ad6a-68595d0fa9f1" shortName="EC4" inObj="CH:double;WSstd:double" relationType="ifclause" outObj="RESULT:string">if (WSstd &lt; 0.1 &amp; CH == 1) {RESULT = 'Activate ultrasonic cleaning';} else {RESULT = 'Normal';}</relation>
<relation id="43e03a25-498c-4317-b8f0-0f429eeb3f8d" shortName="VFT1" inObj="MSL:double;WL:double" relationType="ifclause" outObj="RESULT:string">if (WL &lt; MSL) {RESULT = 'Compensate H measurements by 0.1×tidal range';} else {RESULT = 'Normal';}</relation>
<relation id="4c05103e-bb6b-4780-ba1d-0457dcacb7b3" shortName="FC6" inObj="LVP:double" relationType="ifclause" outObj="RESULT:string">if (LVP == 1) {RESULT = 'Add 0.3×ship_draft to predictions for 30min';} else {RESULT = 'Normal';}</relation>
<relation id="540784ed-e989-4ddd-a1f1-304a2baf6fde" shortName="MEW4" inObj="WED:double;T:double" relationType="ifclause" outObj="RESULT:string">if (WED > 4 &amp; T &lt;= 10) {RESULT = 'Initiate manual verification';} else {RESULT = 'Normal';}</relation>
<relation id="57a64dac-9194-4bf0-add6-10073ae41ea9" shortName="MEW3" inObj="SF:double" relationType="ifclause" outObj="RESULT:string">if (SF >= 3) {RESULT = 'Switch to satellite data and reduce confidence by 30%';} else {RESULT = 'Normal';}</relation>
<relation id="79ae231c-d465-4d98-a12e-cd73eaf3f366" shortName="PC3" inObj="Re:double" relationType="ifclause" outObj="RESULT:string">if (Re > Math.exp(6)) {RESULT = 'Activate turbulence mode correction';} else {RESULT = 'Normal';}</relation>
<relation id="96422f31-56df-456e-ad33-693fac1d193c" shortName="MEW5" inObj="DD:double;WP:double" relationType="ifclause" outObj="RESULT:string">if (WP > 15 &amp; DD &lt; 30) {RESULT = 'Issue rip current alert';} else {RESULT = 'Normal';}</relation>
<relation id="96974e6c-d1b8-48f5-9594-07e982cd0a68" shortName="FC4" inObj="EBB:double" relationType="ifclause" outObj="RESULT:string">if (EBB == 1) {RESULT = 'decrease 8%';} else {RESULT = 'Normal';}</relation>
<relation id="99a7a35e-35b9-44bf-a61a-d1a5adb57d20" shortName="FC5" inObj="SIC:double" relationType="ifclause" outObj="RESULT:string">if (SIC > 60) {RESULT = 'Multiply prediction by max(0.5,1-0.008×ice_thickness)';} else {RESULT = 'Normal';}</relation>
<relation id="b2d1a743-62fd-4f88-b241-387a61db2151" shortName="FC3" inObj="FT:double" relationType="ifclause" outObj="RESULT:string">if (FT == 1) {RESULT = 'Increase predictions by 10%';} else {RESULT = 'Normal';}</relation>
<relation id="b8cca9bb-ba16-429c-beaa-52e8d2b1aa73" shortName="FC1" inObj="WS:double" relationType="ifclause" outObj="RESULT:string">if (WS > 0.05) {RESULT = 'Apply ΔH:-0.2×(H/L-0.05)×H correction';} else {RESULT = 'Normal';}</relation>
<relation id="c0e2ed5e-4429-4fae-8f43-dbe5ca6d2589" shortName="VFT2" inObj="TEM:double" relationType="ifclause" outObj="RESULT:string">if (TEM &lt; -5) {RESULT = 'Multiply measured H by 1/(1-0.02×ice_thickness)';} else {RESULT = 'Normal';}</relation>
<relation id="c99a037a-786d-43fb-a8f4-255fa8b0af4e" shortName="PC1" inObj="AC:double" relationType="ifclause" outObj="RESULT:string">if (AC == 1) {RESULT = 'Cap predicted H at min(0.8×depth, 2×Hs)';} else {RESULT = 'Normal';}</relation>
<relation id="cdf03862-7c67-4506-bdd3-2dcfd803e346" shortName="PC2" inObj="WC:double;WS:double" relationType="ifclause" outObj="RESULT:string">if (WC &lt; 0.8 &amp; WS == 10) {RESULT = 'Recalculate energy input terms';} else {RESULT = 'Normal';}</relation>
<relation id="dc5e0695-91b8-41ff-a924-b21da924eb6b" shortName="EC1" inObj="H:double" relationType="ifclause" outObj="RESULT:string">if (H > 3) {RESULT = 'Shut down wave energy converters and activate hydraulic lock';} else {RESULT = 'Normal';}</relation>
<relation id="ea1d356b-b715-4fb1-b575-a9971d548b13" shortName="EC2" inObj="PToBS:double" relationType="ifclause" outObj="RESULT:string">if (PToBS > 80) {RESULT = 'Release 50m anchor chain';} else {RESULT = 'Normal';}</relation>
<relation id="f65a126c-a72c-4917-8673-16be08243522" shortName="EC3" inObj="PF:double;CP:double;H:double" relationType="ifclause" outObj="RESULT:string">if (H > PF -2 &amp; CP == 3) {RESULT = 'Initiate personnel evacuation';} else {RESULT = 'Normal';}</relation>
<relation id="f779a0a8-80de-4802-84c8-1b93454ac395" shortName="FC2" inObj="PDR:double" relationType="ifclause" outObj="RESULT:string">if (PDR > 5) {RESULT = 'Add pressure_diff×0.05m/hPa to prediction';} else {RESULT = 'Normal';}</relation>
<relation id="fb77ce01-41d9-4522-80e1-e514be745437" shortName="EC5" inObj="P2hrWH:double" relationType="ifclause" outObj="RESULT:string">if (P2hrWH > 1) {RESULT = 'Switch energy storage to discharge mode';} else {RESULT = 'Normal';}</relation>
<relation id="fb9fcfca-7a75-4941-8b9c-52c1d67ed3db" shortName="MEW2" inObj="D:double;H:double" relationType="ifclause" description="H>0.78 and depth&lt;20m （H > 0.78 &amp; D &lt; 20）" outObj="RESULT:string">if (H > 0.78 &amp; D &lt; 20) {RESULT = 'Override predicted wave height to 0.7×depth';} else {RESULT = 'Normal';}</relation>
</relations>
</model>
